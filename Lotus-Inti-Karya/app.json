{
  "expo": {
    "name": "Lotus Inti Karya",
    "slug": "Lotus-Inti-Karya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/liksplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/liklogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shibehibe.LotusIntiKarya"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic":true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a2df1d71-3ca6-4dc9-a1de-755965b438e1"
      }
    }
  }
}
